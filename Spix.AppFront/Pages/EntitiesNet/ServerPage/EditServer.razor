@page "/servers/edit/{Id}"
@attribute [Authorize(Roles = "Usuario")]

@if (Server is null)
{
    <Loading />
}
else
{
    <MudDialog>
        <DialogContent>
            <FormServer Server="Server"
                      IsEditControl="true"
                      OnSubmit="Edit"
                      ReturnAction="Return" />
        </DialogContent>
    </MudDialog>
}


