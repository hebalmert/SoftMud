@page "/nodes/create"
@attribute [Authorize(Roles = "Usuario")]

@if (Node is null)
{
    <Loading />
}
else
{
    <MudDialog Style="width: 80vw;">
        <DialogContent>
            <FormNode Node="Node"
                      IsEditControl="false"
                      OnSubmit="Create"
                      ReturnAction="Return" />
        </DialogContent>
    </MudDialog>
}

