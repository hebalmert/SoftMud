@page "/contractor/edit/{Id:guid}"
@attribute [Authorize(Roles = "Usuario")]

@if (Contractor is null)
{
    <Loading />
}
else
{
    <MudDialog>
        <DialogContent>
            <FormContractor Contractor="Contractor"
                             IsEditControl="true"
                             OnSubmit="Edit"
                             ReturnAction="Return" />
        </DialogContent>
    </MudDialog>
}

