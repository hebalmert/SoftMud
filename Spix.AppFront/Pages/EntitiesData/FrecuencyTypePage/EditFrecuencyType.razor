@page "/frecuencytypes/edit/{Id:int}"
@attribute [Authorize(Roles = "Admin")]

@if (FrecuencyType is null)
{
    <Loading />
}
else
{
    <MudDialog>
        <DialogContent>
            <FormFrecuencyType FrecuencyType="FrecuencyType"
                             OnSubmit="Edit"
                             ReturnAction="Return" />
        </DialogContent>
    </MudDialog>
}