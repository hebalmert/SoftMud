@page "/documenttypes/create"
@attribute [Authorize(Roles = "Usuario")]

@if (DocumentType is null)
{
    <Loading />
}
else
{
    <MudDialog>
        <DialogContent>
            <FormDocumentType DocumentType="DocumentType"
                      OnSubmit="Create"
                      ReturnAction="Return" />
        </DialogContent>
    </MudDialog>
}

